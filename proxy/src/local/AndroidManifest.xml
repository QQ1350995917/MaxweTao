<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.maxwe.tao.proxy.android">

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".mine.LoginActivity"></activity>
        <activity android:name=".mine.RegisterActivity"></activity>
        <activity android:name=".mine.LostActivity"></activity>
        <activity android:name=".mine.ModifyActivity"></activity>
        <activity android:name=".main.MainActivity"></activity>
        <activity android:name=".main.AgentActivity"></activity>
        <activity android:name=".main.TradeActivity"></activity>
    </application>

</manifest>