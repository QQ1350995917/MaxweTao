<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="org.maxwe.tao.android">

    <application
        android:name=".AgentApplication"
        tools:replace="android:name"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.LoginActivity"></activity>
        <activity android:name=".activity.RegisterActivity"></activity>
        <activity android:name=".activity.LostActivity"></activity>
        <activity android:name=".activity.ModifyActivity"></activity>
        <activity
            android:name=".activity.VersionActivity"
            android:theme="@style/AppTheme_activity_dialog"></activity>
        <activity android:name=".main.MainActivity"></activity>
        <activity
            android:name=".code.GenCodeActivity"
            android:theme="@style/AppTheme_activity_dialog"></activity>
        <activity android:name=".code.HistoryActivity"></activity>
        <activity android:name=".agent.TrunkActivity"></activity>
        <activity android:name=".agent.TradeActivity"></activity>
        <activity android:name=".mine.BankActivity"></activity>
        <activity android:name=".mine.RebateActivity"></activity>
    </application>

</manifest>